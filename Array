}

6-
#include <stdio.h>

int main()
{
    int n,u=0;
    int even=0;
    scanf("%d",&n);
    
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    
    for(int i=0;i<n;i++)
    {
        if(a[i]%2==0){
            even++;
        }
    }
    for(int i=0;i<n;i++)
    {
        if(a[i]%2!=0){
            u++;
        }
    }
    
    printf("total even no. are %d\ntotal odd no. are%d",even,u);

    return 0;
}

7-
#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    
    int a[n];
    int b[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    
    for(int i=0;i<n;i++)
    {
        b[i]=a[i];
    }
    for(int i=0;i<n;i++){
        printf("%d,",b[i]);
    }
    

    return 0;
}

