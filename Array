#include <stdio.h>

int main()
{
    int a[10];
    
    for(int i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<10;i++)
    {
        printf("%d",&a[i]);
    }

    return 0;
}

2-
#include <stdio.h>

int main()
{
    int a[10]={1,12,13,-12,-23,13,23,-13,9,98};
    
    for(int i=0;i<10;i++)
    {
        if(a[i]<0){
            printf("%d",a[i]);
        }
    }

    return 0;
}

3-
#include <stdio.h>

int main()
{
    int a[10]={1,12,13,-12,-23,13,23,-13,9,98};
    int b=0;
    for(int i=0;i<10;i++)
    {
        b=b+a[i];
    }
    printf("%d",b);

    return 0;
}

4-
Maximum:


#include<stdio.h>
int main(){
    int arr[45],n,max,i;

    printf("enter n = ");
    scanf("%d",&n);


printf("enter array :\n");
for(i=0;i<n;i++){           //array for taking user input
    scanf("%d",&arr[i]);
}

printf("array is : \n");
for(i=0;i<n;i++){           //array for  value enter by user 
    printf("%d\n",arr[i]);
}

max = arr[0];
for(i=0;i<n;i++){
    if(arr[i]>max){           //for finding maximum element in array
    max = arr[i];
    }
}
printf("maximum elelment in array is = %d",max);    //print the maximum elelment of array
}

Minimum;

#include<stdio.h>
int main(){
    int arr[45],n,min,i;

    printf("enter n = ");
    scanf("%d",&n);


printf("enter array :\n");
for(i=0;i<n;i++){           //array for taking user input
    scanf("%d",&arr[i]);
}

printf("array is : \n");
for(i=0;i<n;i++){           //array for  value enter by user 
    printf("%d\n",arr[i]);
}

min = arr[0];
for(i=0;i<n;i++){
    if(arr[i]<min){           //for finding maximum element in array
    min = arr[i];
    }
}
printf("minimum elelment in array is = %d",min);    //print the maximum elelment of array
}

5-
#include <stdio.h>

int main()
{
   int a[]={1,2,3,4,55},c;
   for(int i=0;i<5;i++){
       for(int j=0;j<i;j++){
           if(a[j]>a[i]){
               c=a[j];
               a[j]=a[i];
               a[i]=c;
           }
       }
       
   }printf("%d",a[3]);

    return 0;
}

