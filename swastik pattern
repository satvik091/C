#include <stdio.h>

int main()
{
    int n,j,i;
    scanf("%d",&n);
    
    for(i=1;i<=n;i++){
        for(j=0;j<=n;j++){
            if(i==(n/2)+1 || j==n/2 +1 || (i==1 && j>=(n/2+1) && (j<=n))||(j==1 && i<=(n/2+1) && (i>=1)) || (i==n && j>=1 && j<=n/2+1) ||  (j==n && i<=n && i>=n/2+1 ) )
            {
                printf("*");
            }
            else{
                printf(" ");
            }
        }
        printf("\n");
    }

    return 0;
}
